{
  "name": "lab3-stackoverflow",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:Boto1/lab3-stackoverflow.git",
  "license": "MIT",
  "scripts": {
    "dev:server": "yarn --cwd ./server dev",
    "dev:client": "yarn --cwd ./client dev",
    "dev": "concurrently -k -p \"[{name}]\" -c \"yellow.bold,cyan.bold\" -n \"client,api\" \"yarn dev:client\" \"yarn dev:server\"",
    "install-deps": "concurrently \"yarn\" \"yarn --cwd client\" \"yarn --cwd server\""
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.4",
    "@vue/cli-plugin-router": "~4.5.0",
    "concurrently": "^6.2.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.0"
  }
}
